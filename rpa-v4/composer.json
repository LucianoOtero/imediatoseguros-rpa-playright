{
    "name": "imediatoseguros/rpa-v4",
    "description": "RPA V4 - Sistema de Automação Robótica de Processos",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RPA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RPA\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
